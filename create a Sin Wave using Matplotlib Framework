import matplotlib.pyplot as plt
import numpy as np

# Generate some sample data
x = np.array([0,10,100])
y = np.sin(x)

# Create a figure and axis object
fig,ax = plt.subplots()

# Plot the data with a red dashed line and square markers
ax.plot(x, y, linestyle = '--', marker = 's', color = 'red')

# Add labels and a title
ax.set_xlabel('X')
ax.set_ylabel('Y')
ax.set_title('Sine Wave')

# Show the plot
plt.show()


